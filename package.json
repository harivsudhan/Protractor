{
	"name": "protractor-automation-framework",
	"description": "Protractor - Testing",
	"scripts": {
		"start": "start webdriver-manager start",
		"test": "protractor protractor.conf.js",
		"chromeDirectTest": "protractor protractor.conf.js --browser=chrome --directConnect=true --cucumberOpts.tags @valid",
		"chromeTest": "start protractor protractor.conf.js --browser=chrome --cucumberOpts.tags @invalid",
		"ieTest": "start protractor protractor.conf.js --browser=ie --cucumberOpts.tags @invalid",
		"firefoxTest": "start protractor protractor.conf.js --browser=firefox --cucumberOpts.tags @valid",
		"crossBrowserTest": "npm run ieTest && npm run firefoxTest && npm run chromeTest",
		"postinstall": "webdriver-manager update --versions.chrome 2.40 && webdriver-manager update --ie32 && npm run start"
	},
	"private": true,
	"dependencies": {
		"@types/chai": "4.1.2",
		"@types/lodash": "4.14.30",
		"@types/node": "10.5.2",
		"bootstrap": "3.3.7",
		"chai": "4.1.2",
		"cucumber": "1.3.3",
		"cucumber-html-reporter": "^4.0.5",
		"fs-extra": "2.1.2",
		"lodash": "^4.17.11",
		"ncp": "^2.0.0",
		"protractor": "5.4.2",
		"protractor-cucumber-framework": "^4.0.9",
		"protractor-multiple-cucumber-html-reporter-plugin": "^1.8.0",
		"selenium-webdriver": "2.53.1",
		"xlsx": "0.14.1"
	},
	"devDependencies": {
		"selenium-standalone": "^6.15.4",
		"ts-node": "7.0.1",
		"typescript": "^2.7.2"
	}
}